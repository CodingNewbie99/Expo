indString":"Call signature","comment":{"shortText":"Gets the behavior of the status and navigation bars when the user swipes or touches the screen.","tags":[{"tag":"platform","text":"android\n"}]},"type":{"type":"reference","typeArguments":[{"type":"reference","name":"Behavior"}],"name":"Promise"}}]},{"name":"getBorderColorAsync","kind":64,"kindString":"Function","signatures":[{"name":"getBorderColorAsync","kind":4096,"kindString":"Call signature","comment":{"shortText":"Gets the navigation bar's border color.","tags":[{"tag":"platform","text":"android\n"},{"tag":"example","text":"\n```typescript\nconst color = await NavigationBar.getBorderColorAsync();\n```\n"}]},"type":{"type":"reference","typeArguments":[{"type":"union","types":[{"type":"reference","name":"ColorValue"},{"type":"literal","value":null}]}],"name":"Promise"}}]},{"name":"getPositionAsync","kind":64,"kindString":"Function","signatures":[{"name":"getPositionAsync","kind":4096,"kindString":"Call signature","comment":{"shortText":"Whether the navigation and status bars float above the app (absolute) or sit inline with it (relative).\nThis value can be incorrect if `androidNavigationBar.visible` is used instead of the config plugin `position` property.","tags":[{"tag":"platform","text":"android\n"}]},"type":{"type":"reference","typeArguments":[{"type":"reference","name":"Position"}],"name":"Promise"}}]},{"name":"getVisibilityAsync","kind":64,"kindString":"Function","signatures":[{"name":"getVisibilityAsync","kind":4096,"kindString":"Call signature","comment":{"shortText":"Get the navigation bar's visibility.","tags":[{"tag":"platform","text":"android\n"},{"tag":"example","text":"\n```typescript\nNavigationBar.setVisibilityAsync(\"hidden\");\n```\n"}]},"type":{"type":"reference","typeArguments":[{"type":"reference","name":"Visibility"}],"name":"Promise"}}]},{"name":"setAppearanceAsync","kind":64,"kindString":"Function","signatures":[{"name":"setAppearanceAsync","kind":4096,"kindString":"Call signature","com