{
  "name": "@expo/docs-generator",
  "version": "0.0.0",
  "private": true,
  "author": "Expo",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && yarn generate",
    "build": "tsc",
    "command": "typedoc --emit --disableSources --hideGenerator --readme none --excludePrivate --excludeProtected",
    "generate": "node build/index.js"
  },
  "devDependencies": {
    "typedoc": "^0.20.32"
  }
}
